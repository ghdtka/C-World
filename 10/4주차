#include<stdio.h>
#include<stdlib.h>
#include<time.h>
int main()
{
	int ar[6][6]={0};
	int i,j;
	int max;
	srand(time(NULL));
	for(i = 0;i<6;i++)
	{
		for(j=0;j<6;j++)
		{
			ar[i][j]=rand()%99+1;
		}
	}
	for(int i=0;i<6;i++)
	{
		for(int j=0;j<6;j++)
		{
			if(ar[i][j]>max)
			{
				max=ar[i][j];
			}
		}
	}
	
	i=0;
	while(i<6)
	{
		j=0;
		while(j<6)
		{
			printf("%3d",ar[i][j]);
			j++;
		}
		printf("\n");
		i++;
	}
	printf("%d",max);
	return 0;
